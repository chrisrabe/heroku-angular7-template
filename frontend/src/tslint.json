{
    "extends": "../tslint.json",
    "rules": {
        "completed-docs": [
            true,
            "functions"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "no-redundant-jsdoc": false,
        "trackBy-function": false
    }
}
